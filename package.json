{
  "name": "npm-addict",
  "displayName": "npm addict",
  "version": "0.9.12",
  "description": "Your daily injection of npm packages",
  "author": "Manuel Vila <mvila@3base.com>",
  "license": "MIT",
  "reveal": false,
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/mvila/npm-addict"
  },
  "scripts": {
    "build": "node index.js build",
    "deploy": "(git checkout --quiet master && test -z \"$(git status --porcelain)\" && git checkout $npm_config_target && git merge master --message \"Merge branch 'master' into '$npm_config_target'\" && git push && git checkout master) || echo \"An error occured (unclean repository?)\"",
    "deploy-production": "npm run deploy --target=production",
    "postinstall": "npm run build",
    "release": "npm run lint && (git checkout --quiet master && test -z \"$(git status --porcelain)\" && npm shrinkwrap && (test -z \"$(git status --porcelain)\" || git commit --all --message \"Run npm shrinkwrap\") && npm version $npm_config_release_type && git push --follow-tags) && npm publish || echo \"An error occurred (uncommitted files?)\"",
    "release-patch": "npm run release --release-type=patch",
    "release-minor": "npm run release --release-type=minor",
    "release-major": "npm run release --release-type=major",
    "start": "node index.js start",
    "lint": "eslint .",
    "test": "echo \"Warning: no test specified\" && exit 0",
    "watch": "node index.js build-and-start --watch"
  },
  "dependencies": {
    "autotrack": "^0.6.3",
    "babel-plugin-transform-async-to-generator": "^6.7.4",
    "babel-plugin-transform-class-properties": "^6.4.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-function-bind": "^6.3.13",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.7.2",
    "babelify": "^7.2.0",
    "better-node-env": "^1.0.1",
    "browserify": "^13.0.0",
    "denodeify": "^1.2.1",
    "easy-aws": "^0.4.2",
    "easy-notifier": "^0.3.2",
    "event-emitter-mixin": "^0.3.1",
    "github-url-to-object": "^2.1.0",
    "idgen": "^2.0.2",
    "isomorphic-fetch": "^2.2.1",
    "json-web-storage": "^1.0.4",
    "koa": "^2.0.0-alpha.3",
    "koa-convert": "^1.2.0",
    "koa-cors": "0.0.16",
    "koa-rewrite": "^1.1.1",
    "koa-router": "^7.0.1",
    "koa-static-cache": "^3.1.6",
    "lodash": "^4.2.1",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.12.0",
    "ncp": "^2.0.0",
    "node-watch": "^0.3.5",
    "object-layer": "^0.4.4",
    "radium": "^0.17.0",
    "radium-starter": "^0.7.8",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-icons": "^1.0.3",
    "rimraf": "^2.5.1",
    "rss": "^1.2.1",
    "sleep-promise": "^1.0.0",
    "twit": "^2.2.3",
    "uglify-js": "^2.6.1",
    "universal-analytics": "^0.3.11",
    "universal-log": "^0.3.8",
    "url-regex": "^3.1.0",
    "watchify": "^3.7.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.0",
    "eslint": "^2.5.3",
    "eslint-config-next": "^0.1.0",
    "eslint-plugin-react": "^4.2.3",
    "node-dev": "^3.1.0"
  }
}
